<h1> Edit Song Form </h1>
<%= form_for(@song) do |f| %>
  <%= f.label :my_song_name %>
  <%= f.text_field :name, value: @song.name %>
  <%= f.label :song_artist %>
  <%= f.text_field :artist_id, value: @song.artist.id %>
  <%= f.label :song_genre %>
  <%= f.text_field :genre_id, value: @song.genre.id %>
  <%= f.submit %>
<% end %>
